{
  "name": "react-bundler",
  "version": "1.0.0",
  "description": "A process to wrap react components for consumption in non-react websites using webpack",
  "main": "wrapper.js",
  "scripts": {
    "example": "node server.js",
    "bundle-js-dev": "webpack --config ./webpack.dev.config.js",
    "bundle-js-prod": "webpack -p --config ./webpack.prod.config.js",
    "bundle-js": "npm run test && npm run bundle-js-dev && npm run bundle-js-prod"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jkdellow/react-bundler"
  },
  "author": "Seven Leaps <sevenleaps@gmail.com> (https://medium.com/@sevenleaps)",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.5.0",
    "babel-core": "^6.5.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.5.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-runtime": "^6.5.0",
    "express": "^4.13.4",
    "react": "^15.0.1",
    "react-dom": "^15.0.0",
    "webpack": "^1.13.0"
  },
  "devDependencies": {
    "webpack-cli": "^3.3.2"
  }
}
