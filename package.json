{
  "name": "react-bundler",
  "version": "1.0.0",
  "description": "A process to wrap react components for consumption in non-react websites using webpack",
  "main": "wrapper.js",
  "scripts": {
    "bundle-js-dev": "webpack --config ./webpack.dev.config.js",
    "bundle-js-prod": "webpack -p --config ./webpack.prod.config.js",
    "bundle-js": "npm run test && npm run bundle-js-dev && npm run bundle-js-prod"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jkdellow/react-bundler"
  },
  "author": "Seven Leaps <sevenleaps@gmail.com> (https://medium.com/@sevenleaps)",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "webpack": "^4.31.0"
  },
  "devDependencies": {
    "webpack-cli": "^3.3.2"
  }
}
